%html
  %head
    %title API Modeling Page
    %meta{:content => "Estimate your Service performance w/o testing", :name => "description"}/
  %body
    %script{:src => "https://cdn.tailwindcss.com"}
    %script{:src => "https://www.gstatic.com/charts/loader.js", :type => "text/javascript"}
